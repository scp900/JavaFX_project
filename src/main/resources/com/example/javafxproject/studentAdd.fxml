<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.javafxproject.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label fx:id="studentAddHeader" text="Enter Student Info"/>
    <TextField fx:id="AddstudentIDInput" promptText="Enter a student ID"/>
    <TextField fx:id="studentNameInput" promptText="Enter the student name"/>
    <TextField fx:id="studentGenderInput" promptText="Enter the student gender"/>
    <TextField fx:id="studentAgeInput" promptText="Enter the student age"/>
    <Button text="Add" onAction="#onAdd"/>
    <Button text="Back" onAction="#onToHome"/>

</VBox>
